<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.sse.app.robot.LeftArm" id="leftArm"></bean>
	<bean class="com.sse.app.robot.RightArm" id="rightArm"></bean>
	
	<bean class="com.sse.app.robot.Wheel" id="wheel">
		<constructor-arg index="0" value="10"></constructor-arg>
		<constructor-arg index="1" value="plave"></constructor-arg>
		<constructor-arg index="2" value="5"></constructor-arg>
	</bean>
	
	<bean class="com.sse.app.robot.Robot" id="robot">
		<property name="leftArm" ref="leftArm"></property>
		<property name="rightArm" ref="rightArm"></property>
		<property name="company"  value="Doosan"></property>
		<property name="age"  value="20"></property>
	</bean>

</beans>
